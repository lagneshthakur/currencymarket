{
    "index": "/index.html",
    "assetGroups": [{
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.css",
          "/*.js",
          "/*.chunk.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }],
    "dataGroups": [{
      "name": "currency-api",
      "urls": [
        "/ticker"
      ],
      "cacheConfig":{
        "strategy": "performance",
        "maxAge": "1d",
        "maxSize": 1000
      }
    }]
  }